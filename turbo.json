{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"outputs": ["dist/**", ".next/**", "public/dist/**"],
			"dependsOn": ["^build"]
		},
		"test": {
			"outputs": ["coverage/**"],
			"dependsOn": []
		},
		"lint": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"lint:fix": {
			"dependsOn": ["^build"],
			"outputs": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx", "**/*.json", "**/*.md"]
		},
		"start": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"dev": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"clean": {
			"cache": false
		},
		"cypress": {
			"cache": false
		},
		"cypress:headless": {
			"cache": false
		}
	},
	"globalDependencies": [".env", "tsconfig.json"]
}
