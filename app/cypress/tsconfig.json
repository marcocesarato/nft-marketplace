{
	"extends": "@packages/ts-config/cypress.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["../*"],
			"@app/*": ["../src/*"],
			"@pages/*": ["../src/pages/*"]
		},
		"esModuleInterop": true,
		"jsx": "react"
	},
	"include": ["**/*.ts", "e2e", "component", "cypress.d.ts"],
	"exclude": ["node_modules"]
}
