{
	"name": "metaverse-marketplace",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": ">=14.17",
		"npm": "please-use-yarn",
		"yarn": ">=1.22"
	},
	"scripts": {
		"dev": "next dev",
		"dev:chain": "npx hardhat node",
		"deploy:local": "npx hardhat run scripts/deploy.js --network localhost",
		"deploy:test": "npx hardhat run scripts/deploy.js --network mumbai",
		"deploy": "npx hardhat run scripts/deploy.js --network matic",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"prepare": "husky install",
		"format": "prettier --write .",
		"static:build": "yarn build && next export",
		"static:serve": "yarn static:build && npx servor out index.html 5000",
		"release": "standard-version"
	},
	"dependencies": {
		"@chakra-ui/icons": "^1.1.7",
		"@chakra-ui/react": "^1.8.6",
