		"react-dom": "17.0.2",
		"react-dropzone": "^12.0.4",
		"react-error-boundary": "^3.1.4",
		"react-icons": "^4.3.1",
		"react-moralis": "^1.3.2",
		"react-query": "^3.34.16",
		"react-ssr-prepass": "^1.5.0",
		"standard-version": "^9.3.2",
