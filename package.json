{
	"name": "metaverse-marketplace",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": ">=14.17",
		"npm": "please-use-yarn",
		"yarn": ">=1.22"
	},
	"scripts": {
		"dev": "next dev",
		"dev:chain": "npx hardhat node",
		"deploy:local": "npx hardhat run scripts/deploy.js --network localhost",
		"deploy:test": "npx hardhat run scripts/deploy.js --network mumbai",
		"deploy": "npx hardhat run scripts/deploy.js --network matic",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"prepare": "husky install",
		"format": "prettier --write .",
		"static:build": "yarn build && next export",
		"static:serve": "yarn static:build && npx servor out index.html 5000"
	},
	"dependencies": {
		"@chakra-ui/css-reset": "^1.1.3",
		"@chakra-ui/icons": "^1.1.7",
		"@chakra-ui/react": "^1.8.6",
		"@emotion/react": "11",
		"@emotion/styled": "11",
		"@metamask/jazzicon": "^2.0.0",
		"@nomiclabs/hardhat-ethers": "^2.0.5",
		"@nomiclabs/hardhat-waffle": "^2.0.3",
		"@openzeppelin/contracts": "^4.5.0",
		"@walletconnect/web3-provider": "^1.7.3",
		"@web3auth/web3auth": "^0.3.2",
		"axios": "^0.26.0",
		"dotenv": "^16.0.0",
		"eslint-plugin-prettier": "^4.0.0",
		"ethereum-waffle": "^3.4.0",
		"ethers": "^5.5.4",
		"focus-visible": "^5.2.0",
		"framer-motion": "6",
		"hardhat": "^2.9.0",
		"history": "^5.3.0",
		"magic-sdk": "^8.0.1",
		"moralis": "^1.3.5",
		"next": "12.1.0",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-dropzone": "^12.0.4",
		"react-error-boundary": "^3.1.4",
		"react-icons": "^4.3.1",
		"react-moralis": "^1.3.2",
		"react-query": "^3.34.16",
		"three": "^0.138.2"
	},
	"devDependencies": {
		"@next/eslint-plugin-next": "^12.1.0",
		"dotenv-webpack": "^7.1.0",
		"eslint": "8.10.0",
		"eslint-config-next": "12.1.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-react-app": "7.0.0",
		"eslint-plugin-flowtype": "8.0.3",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-react": "7.29.2",
		"eslint-plugin-react-hooks": "4.3.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.4",
		"prettier": "^2.5.1"
	}
}
