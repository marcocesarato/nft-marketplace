		"@metamask/jazzicon": "^2.0.0",
		"@nomiclabs/hardhat-ethers": "^2.0.5",
		"@nomiclabs/hardhat-waffle": "^2.0.3",
		"@openzeppelin/contracts": "^4.5.0",
		"@react-three/drei": "^8.16.5",
		"@react-three/fiber": "^7.0.26",
		"@react-three/xr": "^3.4.0",
		"@walletconnect/web3-provider": "^1.7.4",
		"@web3auth/web3auth": "^0.5.1",
		"apexcharts": "^3.33.2",
		"axios": "^0.26.1",
		"cannon-es": "^0.19.0",
		"dotenv": "^16.0.0",
		"eslint-plugin-prettier": "^4.0.0",
		"ethereum-waffle": "^3.4.0",
		"ethers": "^5.6.0",
		"focus-visible": "^5.2.0",
		"framer-motion": "6.2.8",
		"hardhat": "^2.9.1",
		"history": "^5.3.0",
		"magic-sdk": "^8.1.0",
		"mongoose": "^6.2.6",
		"moralis": "^1.3.5",
		"next": "^12.1.0",
		"react": "17.0.2",
